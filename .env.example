# LiveKit Cloud credentials
# Get these from https://cloud.livekit.io
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret

# Google Cloud Configuration
# For Vertex AI Gemini Live API
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=/app/credentials/google-credentials.json

# Legacy Gemini API Key (optional - for non-Vertex setup)
# GOOGLE_API_KEY=your_gemini_api_key

# Agent pool size (pre-warmed bots ready for instant connect)
AGENT_POOL_SIZE=3

# Frontend config (used at build time)
NEXT_PUBLIC_LIVEKIT_URL=wss://your-project.livekit.cloud
NEXT_PUBLIC_BACKEND_URL=http://localhost:8000
